<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        android:id="@+id/pitch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/football_field"
        app:layout_constraintTop_toTopOf="parent" />
    <ImageView
        android:id="@+id/ChatGPT"
        android:src="@drawable/t_shirt"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintWidth_percent="0.1"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintVertical_bias="0.5"
        app:layout_constraintTop_toTopOf="@id/pitch"
        app:layout_constraintBottom_toBottomOf="@id/pitch"
        app:layout_constraintStart_toStartOf="@id/pitch"
        app:layout_constraintEnd_toEndOf="@id/pitch"/>
    <ImageView
        android:id="@+id/MidJourney"
        android:src="@drawable/t_shirt"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintWidth_percent="0.1"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintHorizontal_bias="0.27"
        app:layout_constraintVertical_bias="0.5"
        app:layout_constraintTop_toTopOf="@id/pitch"
        app:layout_constraintBottom_toBottomOf="@id/pitch"
        app:layout_constraintStart_toStartOf="@id/pitch"
        app:layout_constraintEnd_toEndOf="@id/pitch"/>
    <ImageView
        android:id="@+id/MusicFX"
        android:src="@drawable/t_shirt"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintWidth_percent="0.1"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintVertical_bias="0.25"
        app:layout_constraintTop_toTopOf="@id/pitch"
        app:layout_constraintBottom_toBottomOf="@id/pitch"
        app:layout_constraintStart_toStartOf="@id/pitch"
        app:layout_constraintEnd_toEndOf="@id/pitch"/>
    <ImageView
        android:id="@+id/StableDiffusion"
        android:src="@drawable/t_shirt"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintWidth_percent="0.1"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintVertical_bias="0.75"
        app:layout_constraintTop_toTopOf="@id/pitch"
        app:layout_constraintBottom_toBottomOf="@id/pitch"
        app:layout_constraintStart_toStartOf="@id/pitch"
        app:layout_constraintEnd_toEndOf="@id/pitch"/>
    <ImageView
        android:id="@+id/Kandinsky"
        android:src="@drawable/t_shirt_green"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintWidth_percent="0.1"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintHorizontal_bias="0.58"
        app:layout_constraintVertical_bias="0.5"
        app:layout_constraintTop_toTopOf="@id/pitch"
        app:layout_constraintBottom_toBottomOf="@id/pitch"
        app:layout_constraintStart_toStartOf="@id/pitch"
        app:layout_constraintEnd_toEndOf="@id/pitch"/>
</androidx.constraintlayout.widget.ConstraintLayout>


